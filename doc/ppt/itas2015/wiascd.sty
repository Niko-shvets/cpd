\newcommand*{\WIASCD@parse@fontshapedecl}{}
\def\WIASCD@parse@fontshapedecl#1<#2>#3*#4[#5]#6\@nil{%
  \def\WIASCD@fontshapedecl@i{#1}%
  \def\WIASCD@fontshapedecl@ii{#2}%
  \def\WIASCD@fontshapedecl@iii{#3}%
  \def\WIASCD@fontshapedecl@iv{#4}%
  \def\WIASCD@fontshapedecl@v{#5}%
  \def\WIASCD@fontshapedecl@vi{#6}%
}
\newcommand*{\WIASCD@DeclareFontShape@Orig}{}
\let\WIASCD@DeclareFontShape@Orig\DeclareFontShape@
\renewcommand*{\DeclareFontShape@}[6]{%
  \begingroup
    \xdef\WIASCD@fntdesc{#5}%
    \def\WIASCD@tempa{#2}%
    \def\WIASCD@tempb{ptm}\ifx\WIASCD@tempa\WIASCD@tempb
      \expandafter\WIASCD@parse@fontshapedecl#5<>*[]\@nil
      \ifx\WIASCD@fontshapedecl@vi\@empty
        \ifx\WIASCD@fontshapedecl@v\@empty
          \ifx\WIASCD@fontshapedecl@iv\@empty
            \ifx\WIASCD@fontshapedecl@iii\@empty\else
              \ifx\WIASCD@fontshapedecl@ii\@empty\else
                \expandafter\WIASCD@parse@fontshapedecl#5*[]\@nil
                \def\@tempb{-}\ifx\WIASCD@fontshapedecl@ii\@tempb
                  \xdef\WIASCD@fntdesc{<->s*[\WIASCD@tmscale]\WIASCD@fontshapedecl@iii}%
                  \PackageInfo{\filename}{replacing font `#5' by `\WIASCD@fntdesc'}%
      \fi\fi\fi\fi\fi\fi
    \else\def\WIASCD@tempb{ppl}\ifx\WIASCD@tempa\WIASCD@tempb
    \fi\fi
  \endgroup
  \WIASCD@DeclareFontShape@Orig{#1}{#2}{#3}{#4}{\WIASCD@fntdesc}{#6}%
}







