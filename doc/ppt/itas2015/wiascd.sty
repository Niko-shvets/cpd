\newcommand*{\WIASCD@set@logo@filename}[2][wias]{%
  \WIASCD@logo@resetkeys
  \setkeys{WIASCD@logo@key}{#2}%
  \ifcase\csname WIASCD@logo@type@#1\endcsname                  % WIAS logo
    \renewcommand*{\WIASCD@logo@name}{wiaslogo-}%
    \ifnum\WIASCD@logo@year>2009
      \renewcommand*{\WIASCD@logo@year}{2010}%
      \renewcommand*{\WIASCD@logo@addon}{}%
    \else\ifnum\WIASCD@logo@year>2002
      \renewcommand*{\WIASCD@logo@year}{2003}%
      \renewcommand*{\WIASCD@logo@color}{}%
      \renewcommand*{\WIASCD@logo@addon}{}%
    \else
      \renewcommand*{\WIASCD@logo@year}{1995}%
      \renewcommand*{\WIASCD@logo@color}{}%
      \renewcommand*{\WIASCD@logo@lang}{}%
      \renewcommand*{\WIASCD@logo@addon}{}%
    \fi\fi
  \or                                                           % FVB logo
    \renewcommand*{\WIASCD@logo@name}{fvblogo-}%
    \ifnum\WIASCD@logo@year>2006
      \renewcommand*{\WIASCD@logo@year}{2007}%
      \renewcommand*{\WIASCD@logo@color}{}%
      \renewcommand*{\WIASCD@logo@lang}{}%
      \renewcommand*{\WIASCD@logo@addon}{}%
    \else
      \renewcommand*{\WIASCD@logo@year}{1992}%
      \renewcommand*{\WIASCD@logo@color}{}%
      \renewcommand*{\WIASCD@logo@lang}{}%
      \renewcommand*{\WIASCD@logo@addon}{}%
    \fi
  \or                                                           % WGL logo
    \renewcommand*{\WIASCD@logo@name}{wgllogo-}%
    \ifnum\WIASCD@logo@year>2011
      \renewcommand*{\WIASCD@logo@year}{2012}%
    \else
      \renewcommand*{\WIASCD@logo@year}{2003}%
      \renewcommand*{\WIASCD@logo@color}{}%
      \renewcommand*{\WIASCD@logo@lang}{}%
      \renewcommand*{\WIASCD@logo@addon}{}%
    \fi
  \or                                                           % IPBB logo
    \renewcommand*{\WIASCD@logo@name}{ipbb}%
      \renewcommand*{\WIASCD@logo@year}{2008}%
      \renewcommand*{\WIASCD@logo@color}{}%
      \renewcommand*{\WIASCD@logo@lang}{}%
      \renewcommand*{\WIASCD@logo@addon}{}%
  \or                                                           % ABUF logo
    \renewcommand*{\WIASCD@logo@name}{abuflogo-}%
      \renewcommand*{\WIASCD@logo@year}{2013}%
      \renewcommand*{\WIASCD@logo@color}{}%
      \renewcommand*{\WIASCD@logo@lang}{}%
      \renewcommand*{\WIASCD@logo@addon}{}%
  \fi
  \edef\WIASCD@logo@filename{%
    \WIASCD@logo@name%
    \WIASCD@logo@year%
    \WIASCD@logo@addon%
    \WIASCD@logo@color%
    \WIASCD@logo@lang%
  }%
}
